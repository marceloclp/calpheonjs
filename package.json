{
  "name": "calpheonjs",
  "version": "1.0.2",
  "description": "Interface to retrieve data about the Black Desert Online game.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Marcelo Perrella <marceloclperrella@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marceloclp/calpheonjs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "tsnode": "node --require ts-node/register --require tsconfig-paths/register",
    "dev": "nodemon src/index.ts",
    "watch": "tsc --watch",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "pretest": "npm run tsnode ./scripts/setup-tests.ts",
    "test": "jest",
    "test:query": "npm run test src/core/query",
    "test:scraper": "npm run test src/core/scraper",
    "test:scraper:builders": "npm run test src/core/scraper/builders",
    "prepublish": "tsc"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.8",
    "chalk": "^4.1.1",
    "fs": "0.0.1-security",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.1",
    "tsconfig-paths": "^3.9.0"
  },
  "_moduleAliases": {
    "@core": "dist/src/core/",
    "@config": "dist/src/config",
    "@typings": "dist/src/typings",
    "@helpers": "dist/src/helpers",
    "@tests/": "dist/src/test"
  }
}
